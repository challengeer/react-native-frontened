{
  "expo": {
    "name": "Challengeer Beta",
    "slug": "challengeer",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon-beta.png",
    "scheme": "challengeer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.challengeer.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true
    },
    "android": {
      "package": "com.challengeer.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-beta.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#ffffff"
      },
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "enableR8": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#a855f7"
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true,
            "enableR8": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "34e0796d-79e1-4e4a-8634-f10cf3acaa15"
      }
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/34e0796d-79e1-4e4a-8634-f10cf3acaa15"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
